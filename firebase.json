{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source":"/index.html",
        "destination": "/KAMAKURAGU_Quiz.html"
      },
      {
        "source":"/KAMAKURAGU_Quiz.html",
        "destination": "/Kamakura_result.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
